<AnchorPane xmlns:fx="http://javafx.com/fxml"
	prefHeight="390" prefWidth="268">
	<!-- 백그라운드 이미지 -->

	<ImageView id="background" fitHeight="390" fitWidth="268"
		pickOnBounds="true">
		<image>
			<Image url="images/Image_warehousing.png" preserveRatio="true"
				smooth="true" />
		</image>
	</ImageView>

	<!-- 계산 버튼 -->
	<children>
		<Button layoutX="180" layoutY="242" fx:id="btn_I_Reckoning"
			style="-fx-background-color: transparent;">
			<graphic>
				<ImageView fitHeight="22" fitWidth="70"
					pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="images/btn_I_Reckoning.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
	</children>
	<!-- 입고 버튼 -->
	<children>
		<Button layoutX="75" layoutY="320" fx:id="btn_I_Warehousing2"
			style="-fx-background-color: transparent;">
			<graphic>
				<ImageView fitHeight="27" fitWidth="70"
					pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="images/btn_I_Warehousing.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
	</children>
	<!-- 닫기 버튼 -->
	<children>
		<Button layoutX="130" layoutY="320" fx:id="btn_I_Exit4"
			style="-fx-background-color: transparent;">
			<graphic>
				<ImageView fitHeight="27" fitWidth="70"
					pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="images/btn_I_Exit4.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
	</children>

	<!-- 번호 텍스트필드 promptText="ModelName" -->
	<children>
		<TextField layoutX="84" layoutY="168" prefHeight="25"
			prefWidth="100" fx:id="I_number"
			style="-fx-background-color: transparent;">
			<font>
				<Font size="12" />
			</font>
		</TextField>
	</children>
	<!-- 현재수량 텍스트필드 promptText="ModelName" -->
	<children>
		<TextField layoutX="84" layoutY="207" prefHeight="25"
			prefWidth="100" fx:id="txt_I_QuantityOfWarehoused"
			style="-fx-background-color: transparent;">
			<font>
				<Font size="12" />
			</font>
		</TextField>
	</children>
	<!-- 추가수량 텍스트필드 promptText="ModelName" -->
	<children>
		<TextField layoutX="84" layoutY="244" prefHeight="25"
			prefWidth="100" fx:id="txt_I_AdditionalQuantity"
			style="-fx-background-color: transparent;">
			<font>
				<Font size="12" />
			</font>
		</TextField>
	</children>

	<!-- 총수량 텍스트필드 promptText="ModelName" -->
	<children>
		<TextField layoutX="84" layoutY="283" prefHeight="25"
			prefWidth="100" fx:id="txt_I_Total"
			style="-fx-background-color: transparent;">
			<font>
				<Font size="12" />
			</font>
		</TextField>
	</children>
</AnchorPane>

